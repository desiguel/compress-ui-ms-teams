{
  "manifest_version": 2,

  "name": "Compressed MS Teams Styling",
  "description": "Compressed MS Teams Styles Customization",
  "version": "0.1",
  "permissions": [
    "tabs", "https://teams.microsoft.com/*"
  ],
  "web_accessible_resources": [
    "teams-sa.css"
  ],
  "content_scripts": [{
    "js": ["loadcss.js"],
    "matches": ["https://teams.microsoft.com/*"]
  }]
}
